div.tutorialCont
  margin 0 auto
  position relative
  width 1024px
  height 750px

div.step
div.image
img.image
div#ruleTableTutorial
  display none

div.tutorial
  position  absolute
  top       50px
  left      10px
  width     300px
  height    300px
  text-align center

div.tutorial .step
  border 6px outset #B8E9B8
  padding 10px 4px 2px 4px
  background-color #E4F7E4
  
div#ruleTableTutorial
  z-index 5
  position absolute
  width 250px
    
img#eg-MM-MM
img#eg-MM-GB
img@eg-GB-MM
  z-index 5
  position absolute
    
a.help:link,a.help:visited,a.help:hover
  text-decoration none
  color #000000

a.help
  cursor help

div.step img
  vertical-align middle  

span.emph 
  text-decoration underline 
  // font-weight bold
   
span.emphtext
  // font-style italic
  font-weight bold
   
li
  text-align left
  
