.main
  margin 0 auto
  
.task
  margin 0 auto
  position relative
  width   1000px
  height  750px

generalInfoTop = 0px
generalInfoHeight = 60px
yourSignalHeight = 67px
yourReportHeight = 137px

.generalInfo
  position absolute
  top       generalInfoTop
  left      0px
  width     250px
  padding   0px 0px 0px 0px
  
.task-yourSignal
  position  absolute
  top       generalInfoTop + generalInfoHeight
  left      35px
  width     180px
  height    60px
  padding   5px 0px 0px 10px
  border-left 2px dashed gray
  border-right 2px dashed gray
  border-top 2px dashed gray

.task-yourReport
  position  absolute
  top       generalInfoTop + generalInfoHeight + yourSignalHeight
  left      35px
  width     180px
  padding   3px 0px 5px 10px
  border-left 2px dashed gray
  border-right 2px dashed gray
  border-bottom 2px dashed gray

.ruleTable
  z-index   0
  position  absolute
  top       generalInfoTop + generalInfoHeight + yourSignalHeight + yourReportHeight
  left      0px
  width     250px
  padding   10px 0px 0px 0px

resultTableHeaderHeight = 135px
resultTableHeaderTop = 0px

.resultTableHeader
  position absolute
  top      resultTableHeaderTop
  left      280px

.resultTable
  z-index   0
  position  absolute
  top       resultTableHeaderTop + resultTableHeaderHeight
  left      280px
  height    300px
  overflow-y scroll
  
  

